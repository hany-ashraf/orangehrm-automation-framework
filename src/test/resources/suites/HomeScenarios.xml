<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Running positive scenarios on main list of home page">
    <parameter name="browser" value=""/>
    <parameter name="URL" value="https://opensource-demo.orangehrmlive.com/web/index.php/auth/login"/>
    <test verbose="3" preserve-order="true" name="loop in all main list of home page">
        <classes>
            <class name="testcases.TC02_HomePage">
                <methods>
                    <include name="validateOnAllTaps_P"/>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true" name="Create PIM and Admin !">
        <classes>
            <class name="testcases.TC02_HomePage">
                <methods>
                    <include name="validateCreateNewPIM_P"/>
                    <include name="validateCreateAdmin_P"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>
